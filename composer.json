{
  "name": "afragen/git-updater-dependency-loader",
  "description": "Add plugin card data for non-dot org dependencies, Git Updater",
  "type": "wordpress-plugin",
  "license": "MIT",
  "authors": [
    {
      "name": "Andy Fragen",
      "email": "andy@thefragens.com",
      "homepage": "https://thefragens.com",
      "role": "Developer"
    }
  ],
  "support": {
    "issues": "https://github.com/afragen/git-updater-dependency-loader/issues",
    "source": "https://github.com/afragen/git-updater-dependency-loader"
  },
  "prefer-stable": true,
  "require": {
    "php": ">=7.2",
    "afragen/add-plugin-dependency-api": "^0"
  },
  "require-dev": {
    "dealerdirect/phpcodesniffer-composer-installer": "^0.7.0",
    "squizlabs/php_codesniffer": "3.6.0",
    "wp-coding-standards/wpcs": "~2.3.0"
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
